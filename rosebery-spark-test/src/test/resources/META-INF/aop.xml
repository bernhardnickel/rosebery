<aspectj>
    <aspects>
        <concrete-aspect name="at.ac.tuwien.infosys.rosebery.spark.FunctionRuntimePerformanceAspect" extends="at.ac.tuwien.infosys.rosebery.common.aspect.AbstractRuntimePerformanceAspect">
            <pointcut name="scope" expression="execution(* org.apache.spark.api.java.function.Function.call(..))" />
        </concrete-aspect>
    </aspects>
    <weaver options="-verbose">
        <exclude within="scala..*" />
        <exclude within="org.spark-project..*" />
        <exclude within="org.apache.spark..*" />
    </weaver>
</aspectj>